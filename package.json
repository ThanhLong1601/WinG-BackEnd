{
  "name": "wing_backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ThanhLong1601/WinG-BackEnd.git",
  "author": "ThanhLong <thanhlong160100@gmail.com>",
  "license": "MIT",
  "description": "WinG-Backend",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/migration -d ./src/data-source.ts -p",
    "migration:run": "yarn typeorm migration:run -d ./src/data-source.ts",
    "migration:revert": " yarn typeorm migration:revert -d ./src/data-source.ts"
  },
  "dependencies": {
    "bcryptjs": "^3.0.1",
    "body-parser": "^1.20.3",
    "celebrate": "^15.0.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-decorators": "^1.2.1",
    "form-data": "^4.0.2",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cliui": "7.0.4",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "yargs": "16.2.0"
  }
}
